programa {
  funcao inicio() {
    
inteiro i, num, termo1, termo2, proximoTermo 



        escreva("Digite um número: ")
        leia(num)
      
    termo1 = 1
    termo2 = 1


    escreva("Os primeiros  " , num, " termos da sequência de Fibonacci são: ")

      para (i = 1; i <= num; i++) {
      escreva(termo1, " ")
      proximoTermo = termo1 + termo2
      termo1 = termo2
      termo2 = proximoTermo
        
        
          
      }
        
    
        
      }


      }
    }
